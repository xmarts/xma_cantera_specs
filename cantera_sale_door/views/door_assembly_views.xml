<odoo>
  <data>

        <record id="door_assembly_view_tree" model="ir.ui.view">
            <field name="name">door.assembly.view.tree</field>
            <field name="model">door.assembly</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Preparación de Ensamble">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_assembly" model="ir.actions.act_window">
            <field name="name">Preparación de Ensamble</field>
            <field name="res_model">door.assembly</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="protections_indicator" name="proteciones" parent="offering_menu" sequence="10"/>
            <menuitem id="door_assembly_sub_menu" name="Preparación de Ensamble" action="action_door_assembly" parent="cantera_sale_door.protections_indicator" sequence="3"/>

  </data>
</odoo>