<odoo>
  <data>

        <record id="door_color_view_tree" model="ir.ui.view">
            <field name="name">door.color.view.tree</field>
            <field name="model">door.color</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Color">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_color" model="ir.actions.act_window">
            <field name="name">Color</field>
            <field name="res_model">door.color</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_color_sub_menu" name="Color" action="action_door_color" parent="cantera_sale_door.head_indicator" sequence="2"/>

  </data>
</odoo>