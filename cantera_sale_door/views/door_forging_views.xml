<odoo>
  <data>

        <record id="door_forging_view_tree" model="ir.ui.view">
            <field name="name">door.forging.view.tree</field>
            <field name="model">door.forging</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Forja">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_forging" model="ir.actions.act_window">
            <field name="name">Forja</field>
            <field name="res_model">door.forging</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_forging_sub_menu" name="Forja" action="action_door_forging" parent="cantera_sale_door.general_indicator" sequence="4"/>

  </data>
</odoo>