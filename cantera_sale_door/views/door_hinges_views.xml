<odoo>
  <data>

        <record id="door_hinges_view_tree" model="ir.ui.view">
            <field name="name">door.hinges.view.tree</field>
            <field name="model">door.hinges</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Bisagras">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_hinges" model="ir.actions.act_window">
            <field name="name">Bisagras</field>
            <field name="res_model">door.hinges</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_hinges_sub_menu" name="Bisagras" action="action_door_hinges" parent="cantera_sale_door.frame_indicator" sequence="2"/>

  </data>
</odoo>