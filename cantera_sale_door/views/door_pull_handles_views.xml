<odoo>
  <data>

        <record id="door_pull_handles_view_tree" model="ir.ui.view">
            <field name="name">door.pull.handles.view.tree</field>
            <field name="model">door.pull.handles</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Jaladeras">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_pull_handles" model="ir.actions.act_window">
            <field name="name">Jaladeras</field>
            <field name="res_model">door.pull.handles</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="pull_handles_indicator" name="Pull Handles" parent="offering_menu" sequence="5"/>
            <menuitem id="door_pull_handles_sub_menu" name="Jaladeras" action="action_door_pull_handles" parent="pull_handles_indicator" sequence="1"/>

  </data>
</odoo>