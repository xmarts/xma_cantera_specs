<odoo>
  <data>

        <record id="door_radius_view_tree" model="ir.ui.view">
            <field name="name">door.radius.view.tree</field>
            <field name="model">door.radius</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Radius">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_radius" model="ir.actions.act_window">
            <field name="name">Radius</field>
            <field name="res_model">door.radius</field>
            <field name="view_mode">tree</field>
        </record>

        
         <menuitem id="door_radius_sub_menu" name="Radius" action="action_door_radius" parent="cantera_sale_door.glass_specs_indicator" sequence="3"/>
  </data>
</odoo>