<odoo>
  <data>

        <record id="door_position_view_tree" model="ir.ui.view">
            <field name="name">door.position.view.tree</field>
            <field name="model">door.position</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Posicion del Fijo">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_position" model="ir.actions.act_window">
            <field name="name">Posicion del Fijo</field>
            <field name="res_model">door.position</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="door_position_sub_menu" name="Posicion del Fijo" action="action_door_position" parent="cantera_sale_door.Sidelight_indicator" sequence="2"/>

  </data>
</odoo>