<odoo>
  <data>

        <record id="door_latch_view_tree" model="ir.ui.view">
            <field name="name">door.latch.view.tree</field>
            <field name="model">door.latch</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Picaportes">
                    <field name="name"/>
                    <field name="price"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_latch" model="ir.actions.act_window">
            <field name="name">Picaportes</field>
            <field name="res_model">door.latch</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="door_latch_sub_menu" name="Picaportes" action="action_door_latch" parent="cantera_sale_door.accessories_indicator" sequence="3"/>

  </data>
</odoo>