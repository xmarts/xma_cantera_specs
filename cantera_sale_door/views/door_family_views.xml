<odoo>
  <data>

        <record id="door_family_view_tree" model="ir.ui.view">
            <field name="name">door.family.view.tree</field>
            <field name="model">door.family</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Familia de Producto">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_family" model="ir.actions.act_window">
            <field name="name">Familia de Producto</field>
            <field name="res_model">door.family</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_family_sub_menu" name="Familia de Producto" action="action_door_family" parent="cantera_sale_door.head_indicator" sequence="7"/>

  </data>
</odoo>