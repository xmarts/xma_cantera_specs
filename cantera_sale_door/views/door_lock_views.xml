<odoo>
  <data>

        <record id="door_lock_view_tree" model="ir.ui.view">
            <field name="name">door.lock.view.tree</field>
            <field name="model">door.lock</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Modelo de Cerradura">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_lock" model="ir.actions.act_window">
            <field name="name">Modelo de Cerradura</field>
            <field name="res_model">door.lock</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="door_lock_sub_menu" name="Modelo de Cerradura" action="action_door_lock" parent="cantera_sale_door.Deadbolt_indicator" sequence="2"/>

  </data>
</odoo>