<odoo>
  <data>

        <record id="door_traslape_view_tree" model="ir.ui.view">
            <field name="name">door.traslape.view.tree</field>
            <field name="model">door.traslape</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Traslape Internas">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_traslape" model="ir.actions.act_window">
            <field name="name">Traslape</field>
            <field name="res_model">door.traslape</field>
            <field name="view_mode">tree</field>
        </record>

        <menuitem id="frame_brickmolding_indicator" name="Frame Brickmolding" parent="offering_menu" sequence="4"/>
            <menuitem id="door_traslape_sub_menu" name="Traslape" action="action_door_traslape" parent="frame_brickmolding_indicator" sequence="1"/>
        
  </data>
</odoo>