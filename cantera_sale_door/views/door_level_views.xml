<odoo>
  <data>

        <record id="door_level_view_tree" model="ir.ui.view">
            <field name="name">door.level.view.tree</field>
            <field name="model">door.level</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Nivel de Complejidad">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_level" model="ir.actions.act_window">
            <field name="name">Nivel de Complejidad</field>
            <field name="res_model">door.level</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_level_sub_menu" name="Nivel de Complejidad" action="action_door_level" parent="cantera_sale_door.head_indicator" sequence="3"/>

  </data>
</odoo>