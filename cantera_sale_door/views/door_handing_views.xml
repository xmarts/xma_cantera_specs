<odoo>
  <data>

        <record id="door_handing_view_tree" model="ir.ui.view">
            <field name="name">door.handing.view.tree</field>
            <field name="model">door.handing</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Handing">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_handing" model="ir.actions.act_window">
            <field name="name">Handing</field>
            <field name="res_model">door.handing</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_handing_sub_menu" name="Handing" action="action_door_handing" parent="cantera_sale_door.general_indicator" sequence="3"/>

  </data>
</odoo>