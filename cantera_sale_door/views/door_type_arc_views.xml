<odoo>
  <data>

        <record id="door_type_arc_view_tree" model="ir.ui.view">
            <field name="name">door.type.arc.view.tree</field>
            <field name="model">door.type.arc</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Tipo de Arco de Puerta">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_type_arc" model="ir.actions.act_window">
            <field name="name">Tipo de Arco de Puerta</field>
            <field name="res_model">door.type.arc</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_type_arc_sub_menu" name="Tipo de Arco de Puerta" action="action_door_type_arc" parent="cantera_sale_door.general_indicator" sequence="6"/>

  </data>
</odoo>