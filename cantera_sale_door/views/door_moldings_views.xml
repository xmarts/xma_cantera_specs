<odoo>
  <data>

        <record id="door_molding_view_tree" model="ir.ui.view">
            <field name="name">door.molding.view.tree</field>
            <field name="model">door.molding</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                    <field name="price_sd" widget="monetary"/> 
                    <field name="price_dd" widget="monetary"/> 
                    <field name="price_bifolds" widget='monetary'/> 
                </tree>
            </field>
        </record>

        <record id="action_door_molding" model="ir.actions.act_window">
            <field name="name">Molduras</field>
            <field name="res_model">door.molding</field>
            <field name="view_mode">tree</field>
        </record>

        
            <menuitem id="door_molding_sub_menu" name="Molduras" action="action_door_molding" parent="frame_brickmolding_indicator" sequence="1"/>
            
  </data>
</odoo>