<odoo>
  <data>

        <record id="door_suffix_view_tree" model="ir.ui.view">
            <field name="name">door.suffix.view.tree</field>
            <field name="model">door.suffix</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Sufijo">
                    <field name="name"/>
                    <field name="price" widget="monetary"/> 
                </tree>
            </field>
        </record>

        <record id="action_door_suffix" model="ir.actions.act_window">
            <field name="name">Sufijo</field>
            <field name="res_model">door.suffix</field>
            <field name="view_mode">tree</field>
        </record>

        
        <menuitem id="door_suffix_sub_menu" name="Sufijo" action="action_door_suffix" parent="cantera_sale_door.head_indicator" sequence="4"/>

  </data>
</odoo>